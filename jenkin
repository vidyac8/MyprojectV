pipeline {
	agent any
	tools {
    	maven 'local_maven'
	}
	stages {
    	stage('clean') {
        	steps {
        	sh "mvn clean"  	 
        	}
    	}
	
    	stage('Build') {
        	steps {
        	sh "mvn compile"  	 
        	}
    	}
   	 
    	stage("Unit test") {          	 
        	steps {  	 
            	sh "mvn test"          	 
        	}
	}

}
}
